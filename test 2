<script>
			function retryDownload(script, url, maxRetries) {
				if (!maxRetries) {
					maxRetries = 5;
				}
				if (script.retryCount == undefined) {
					script.retryCount = 0;
				}
				if (script.retryCount < maxRetries) {
					script.retryCount++;
					setTimeout(function () {
						script.retryCount++;
						setTimeout(function () {
							// Create a new script tag with the same URL and event handler
							const newScript = document.createElement('script');
							newScript.src = url;
							newScript.onerror = function () {
								retryDownload(newScript, url, maxRetries);
							};
							newScript.retryCount = script.retryCount;
							// Replace the old script tag with the new one in the DOM
							script.parentNode.replaceChild(newScript, script);
						}, 2000);
					});
				}
			}
		</script>
